# Volume Control
"pactl set-sink-volume 0 +5%"
  XF86AudioRaiseVolume

"pactl set-sink-volume 0 -5%"
  XF86AudioLowerVolume

"pactl set-sink-mute 0 toggle"
  XF86AudioMute

# Launch Terminal
"/usr/local/bin/st"
  Alt+Return

# Launch Video with MPV
"~/.local/bin/mpvload"
  Alt+  m

## dmenu commands ##

# dmenu
"dmenu_run -p Run"
  Alt+p

# Connect to Network
"~/.local/bin/netcon"
  Alt+ c

# Disconect from Network
"~/.local/bin/netdiscon"
  Alt+ n

# Switch Monitor Setup
"~/.local/bin/monitor"
Alt+Shift+s

# Mount Drives
"~/.local/bin/usbmount"
  Alt+Shift +  m

# Unmount Drives
"~/.local/bin/usbunmount"
  Alt+Shift +  u

# Poweroff Drive (Safely remove)
"~/.local/bin/usbpoweroff"
  Alt+Shift +  p

# Open Video
"~/.local/bin/openvideo"
  Alt+ v

# Open Images
"~/.local/bin/openimage"
  Alt+ i

# Poweroff/Reboot
"~/.local/bin/poweroffreboot"
  Alt+ l

## Misc

# Calendar
"st -e calcurse"
  Alt+Shift+ c

# Take Screenshot
"scrot ~/Pictures/screenshot-%Y-%m-%d-%T.png"
  Print
